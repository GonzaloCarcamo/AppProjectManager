<%= form_with(model: project, local: true) do |f| %>

<div class="form-group">
  <%= f.label :name %>
  <%= f.text_field :name, class:'form-control' %>
</div>

<div class="form-group">
  <%= f.label :description %>
  <%= f.text_area :description, class:'form-control' %>
</div>

<div class="form-group">
  <%= f.label :start_date %>
  <%= f.date_select :start_date, class:'form-control', start_year: 2020, end_year: 2022 %>
</div>

<div class="form-group">
  <%= f.label :end_date %>
  <%= f.date_select :end_date, class:'form-control', start_year: 2020, end_year: 2022 %>
</div>

<div>
    <%= f.label :state %>
    <%= f.select :state, options_for_select(
    ['proposal','in progress', 'finished']),
    :include_blank => 'choose state' %>

</div>


<br>

<div class="form-group">

  <%= f.submit 'Save', class:'form-control' %>

</div>

<% end %>
